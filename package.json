{
  "name": "menhera-tools",
  "version": "3.0.0",
  "description": "Um reposit√≥rio com ferramentas para auxiliar a MenheraBot",
  "private": true,
  "scripts": {
    "build:no-plugin": "lerna exec --parallel yarn run build",
    "start": "lerna exec --parallel yarn run start",
    "logger": "yarn workspace @menhera-tools/logger",
