  "dependencies": {
    "typescript": "^4.1.2"
  },
  "workspaces": {
    "hoistDependencies": true,
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "@menhera-tools/eslint-config": "*",
    "lerna": "^4.0.0"
