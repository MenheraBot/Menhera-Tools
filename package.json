    "bot": "yarn workspace @menhera-tools/amandinha",
    "lint": "yarn workspace @menhera-tools/eslint-config",
    "run:lint": "eslint --fix .",
    "typecheck": "lerna exec --parallel yarn run typecheck"
  },
  "repository": {
    "type": "git",
